{
    "name": "pistol88/yii2-auto-catalog",
    "description": "yii2-auto-catalog it's a module for site auto catalog backend",
    "keywords": ["yii2", "yii 2", "application", "backend", "crud", "auto", "catalog", "module", "pistol88"],
    "homepage": "https://github.com/pistol88/yii2-auto-catalog",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/pistol88/yii2-auto-catalog"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "pistol88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "2amigos/yii2-grid-view-library": "*",
        "pistol88/yii2-gallery": "*",
        "pistol88/yii2-field": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-select2": "*",
        "voskobovich/yii2-many-many-behavior": "^3.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "pistol88\\autocatalog\\": ""
        }
    },
    "extra": {
        "bootstrap": "pistol88\\autocatalog\\Bootstrap"
    }
}
